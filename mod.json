{
	"geode": "4.6.3",
	"gd": {
		"win": "2.2074",
		"android": "2.2074",
		"mac": "2.2074",
		"ios": "2.2074"
	},
	"id": "naxrin.better_pui",
	"name": "Better Platform & Practice Config UI",
	"version": "v3.1.0",
	"developer": "naxrin",
	"description": "Optimized Platform/Practice Buttons config UI",
	"resources": {
		"sprites": [
			"resources/*"
		]
	},
	"dependencies": {
		"geode.node-ids": {
			"version": "1.20.1",
			"importance": "required"
	    }
	},
	"settings": {
		"bg-color": {
			"name": "Background Color",
			"description": "0~100 Default to 85",
			"type": "color",
			"default": "000000"
		},
        "bg-opacity": {
			"name": "Background Opacity",
			"description": "0~100 Default to 85",
			"type": "int",
			"default": 85,
			"max": 100,
			"min": 0,
			"control": {
				"input": true,
				"slider-step": 1
			}
		}
	}
}